    <div class="carousel">
      <div class="slideshow-container">

        <div class="mySlides fade">
          <a href="/images/disc.jpg" class="href">
            <div class="numbertext">1 / 3</div>
            <img src="/images/4697082.jpg" style="width:100%">
            <div class="text">~we could~</div>
          </a>
        </div>

        <div class="mySlides fade">
          <a
            href="https://dynamic.zacdn.com/VVb7-Wwxseiwbvkk8aC4YItNCE8=/fit-in/x/filters:quality(80):fill(ffffff)/https://static-id.zacdn.com/cms/cw10/996x500_HERO__(1).jpg">
            <div class="numbertext">2 / 3</div>
            <img src="/images/20210310_121047.png" style="width:100%">
            <div class="text">~share love~</div>
          </a>
        </div>

        <div class="mySlides fade">
          <a
            href="https://dynamic.zacdn.com/qRNLA4rmW2PWlSw1xI5dhCaKGDA=/fit-in/x/filters:quality(80):fill(ffffff)/https://static-id.zacdn.com/cms/cw10/996x500_hero_SHOPINMAR.jpg">
            <div class="numbertext">3 / 3</div>
            <img src="/images/carousel-instagram-adalah.jpg" style="width:100%">
            <div class="text">~To everyone~</div>
          </a>
        </div>

        <a class="prev" onclick="plusSlides(-1)">&#10094;</a>
        <a class="next" onclick="plusSlides(1)">&#10095;</a>
      </div>
      <br>

      <div style="text-align:center">
        <span class="dot" onclick="currentSlide(1)"></span>
        <span class="dot" onclick="currentSlide(2)"></span>
        <span class="dot" onclick="currentSlide(3)"></span>
      </div>
    </div>
    <div class="ads">
      <% data.forEach(item=> { %>
        <a href="/detail/<%= item.id %>" style="text-decoration: none;">
          <div class="item-ads">
            <img src="/images/ads/<%= item.pictures[0] %>" alt="">
            <h2>
              <%= formatter.format(item.price) %>
            </h2>
            <p>
              <%= item.title %>
            </p>
          </div>
        </a>
        <% }) %>
    </div>
    
